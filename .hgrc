# Default Configuration for Lantiq's Mercurial Distribution
[ui]
username = <USER and EMAIL>


[diff]
git = true

[web]
style = gitweb

[auth]
mts.schemes = https
mts.prefix = mts-chd.intel.com
mts.username = $USER
mts.password = <PASSWORD>

[extensions]
#cc_importucm =
color =
extdiff =
graphlog =
# hgshelve = 
# shelve = /nfs/iil/proj/chdsw/dev/common/mercurial/hgshelve/hgshelve.py
shelve =
#importfs = /nfs/iil/proj/chdsw/dev/common/mercurial/hg-importfs/importfs.py
importfs =
mercurial_keyring = !
# onsub = /nfs/iil/proj/chdsw/dev/common/mercurial/onsub/onsub.py
onsub = /opt/ugw/wcci_tools/hg-env/extensions/onsub/onsub.py
progress =
# prompt = 
# projrc = /nfs/iil/proj/chdsw/dev/common/mercurial/projrc1.py
projrc = /opt/ugw/wcci_tools/hg-env/extensions/projrc/projrc1.py
purge =
rebase =
record =
# snap = 
# subpaths = 
versions = !
highlight =
relink =
snap = !
hgext.snap=!
mq =
#hgsubversion = ~/hgsubversion/hgsubversion
convert = 
# hggit = /tmp2/meydav/experimental/hg-git/hggit
#fastimport = /tmp2/meydav/experimental/hg-fastimport/hgfastimport
histedit = 
#~/durin42-histedit-aea051d162a7/hg_histedit.py

[projrc]
servers = https://mts-chd.intel.com/*
confirm = false

[http_proxy]
no = mts-chd.intel.com
host = proxy-us.intel.com:912

[alias]
clog = !hg log -X $(hg root)/.hgtags
clone = clone --uncompressed


