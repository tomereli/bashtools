[user]
	name = <NAME>
	email = <EMAIL>

[core]
	gitProxy = git-proxy
	gitproxy = none for intel.com
	editor = vi
	autocrlf = input
	pager = cat
	excludesfile = /home/tbeliyah/.gitignore

